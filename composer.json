{
    "name": "fixwa/http-client-collection-generator",
    "description": "Generate Insomnia(r) collection.",
    "keywords": ["postman", "testfully", "insomnia", "rest-client", "generator", "helper", "dev"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo",
            "email": "fixwah@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Fixwa\\HttpClientCollectionGenerator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fixwa\\HttpClientCollectionGenerator\\Tests\\": "./tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    }
}
